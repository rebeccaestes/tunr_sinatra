<h4><a href="/artists/<%= @id %>/edit">Edit This Info</a></h4>

<h2><%= @artist.name %></h2>

<img src=" <%= @artist.photo_url %>" alt=" <%= @artist.name %>">

<p>The <%= @artist.nationality %> artist <strong><%= @artist.name %></strong>'s discography includes;</p>

<% albums = [] %>

<ul>
<% @songs.each do |song| %>
  <% albums.push(song.album) %>
  <li><%= song.title %></li>
<% end %>
</ul>

<% albums = albums.uniq%>
